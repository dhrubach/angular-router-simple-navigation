{
  "name": "angular-router-simple-navigation",
  "version": "0.0.1",
  "description": "simple navigation system using angular router with multiple levels",
  "scripts": {
    "rimraf": "./node_modules/.bin/rimraf",
    "webpack": "./node_modules/.bin/webpack",

	"build": "npm run rimraf -- dist && npm run webpack -- --colors --progress --display-error-details --display-cached",
    "build:watch": "npm run build -- --watch",
	"build:server": "./node_modules/.bin/webpack-dev-server --debug --colors --config ./webpack.config.js",

	"serve": "./node_modules/.bin/http-server ./dist -p 8080 -d false -o"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhrubach/angular-router-simple-navigation.git"
  },
  "keywords": [
    "angular",
    "angular router"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dhrubach/angular-router-simple-navigation/issues"
  },
  "homepage": "https://github.com/dhrubach/angular-router-simple-navigation#readme",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-angular2": "0.0.2",
    "babel-preset-env": "^1.5.1",
    "css-loader": "^0.28.3",
    "custom-tslint-formatters": "^2.1.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "http-server": "^0.10.0",
    "imports-loader": "^0.7.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.1",
    "ts-loader": "^2.1.0",
    "tslint": "^5.3.2",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.3.3",
    "url-loader": "^0.5.8",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "@angular/common": "^4.1.3",
    "@angular/compiler": "^4.1.3",
    "@angular/core": "^4.1.3",
    "@angular/forms": "^4.1.3",
    "@angular/http": "^4.1.3",
    "@angular/platform-browser": "^4.1.3",
    "@angular/platform-browser-dynamic": "^4.1.3",
    "@angular/router": "^4.1.3",
    "babel-runtime": "^6.23.0",
    "bootstrap": "^4.0.0-alpha.6",
